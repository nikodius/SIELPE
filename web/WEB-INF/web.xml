<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>Login</servlet-name>
        <servlet-class>com.sielpe.controller.Login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Elecciones</servlet-name>
        <servlet-class>com.sielpe.controller.Elecciones</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Votacion</servlet-name>
        <servlet-class>com.sielpe.controller.Votacion</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Usuarios</servlet-name>
        <servlet-class>com.sielpe.controller.Usuarios</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Login</servlet-name>
        <url-pattern>/Login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Elecciones</servlet-name>
        <url-pattern>/Elecciones</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Votacion</servlet-name>
        <url-pattern>/Votacion</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Usuarios</servlet-name>
        <url-pattern>/Usuarios</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
